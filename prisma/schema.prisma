datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated"
}

model MetroNetworkStatus {
  id        Int      @id @default(autoincrement())
  info      Json
  info_hash String
  overrides Json?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("metro_network_status")
}
